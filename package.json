{
  "name": "doorglass",
  "version": "1.0.0",
  "description": "door glass effect",
  "main": "doorglass.js",
  "bin": {
    "gd-watch": "bin/watch.js"
  },
  "scripts": {
    "test": "tap test/*.js",
    "dev": "live-server --open=example/index.htm --ignore=node_modules/",
    "predev": "bin/watch.js --debug 'npm run build:css'",
    "build:css": "postcss -u autoprefixer -u cssnano example/**/*.src.css > example/css/all.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotnetCarpenter/doorglass.git"
  },
  "author": "dotnetCarpenter <jon.ronnenberg@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dotnetCarpenter/doorglass/issues"
  },
  "homepage": "https://github.com/dotnetCarpenter/doorglass#readme",
  "dependencies": {
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "babel": "^5.8.23",
    "chokidar": "^1.0.5",
    "cssnano": "^3.0.1",
    "live-server": "^0.8.1",
    "postcss-cli": "^2.1.0",
    "shelljs": "^0.5.3",
    "tap": "^1.4.0"
  }
}
